
public class Methodoverload {

	// Method with two integer parameters
    void add(int a, int b) {
        System.out.println("Sum (int, int): " + (a + b));
    }

    // Overloaded method with three integer parameters
    void add(int a, int b, int c) {
        System.out.println("Sum (int, int, int): " + (a + b + c));
    }

    // Overloaded method with double parameters
    void add(double a, double b) {
        System.out.println("Sum (double, double): " + (a + b));
    }
	public static void main(String[] args) {
		Methodoverload  obj = new OverloadingExample();
        
        obj.add(5, 10);         // Calls method with (int, int)
        obj.add(3, 7, 2);       // Calls method with (int, int, int)
        obj.add(4.5, 2.3);   

	}

}
