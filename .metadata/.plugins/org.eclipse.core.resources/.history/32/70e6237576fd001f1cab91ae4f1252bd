// Car Class (without toString())
class Car {
    private String brand;
    private String model;
    private int year;

    // Constructor
    public Car(String brand, String model, int year) {
        this.brand = brand;
        this.model = model;
        this.year = year;
    }
}

// Main Class
public class WithoutToStringExample {
    public static void main(String[] args) {
        // Creating Car objects
        Car car1 = new Car("Toyota", "Camry", 2023);
        Car car2 = new Car("Honda", "Civic", 2022);

        // Printing objects
        System.out.println(car1); // Prints memory reference
        System.out.println(car2); // Prints memory reference
    }
}
