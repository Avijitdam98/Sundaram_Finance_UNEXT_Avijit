package com.collections;

import java.util.Stack;

public class LearnArrayList {

    public static void main(String[] args) {
        Stack<Integer> str = new Stack<>(); // Corrected variable name formatting
        str.add(10);
        str.add(20);
        str.add(30);

        System.out.println(str);             // Prints the stack
        System.out.println(str.peek());      // Shows the top element without removing it
        System.out.println(str.empty());     // Checks if stack is empty
        System.out.println(str.size());      // Returns the size of stack
        System.out.println(str.contains(10)); // Checks if stack contains 10

        // Corrected loop to use .size() instead of .length
        for (int i = 0; i < str.size(); i++) {
            System.out.println(str.get(i)); // Prints elements correctly
        }
    }
}
