// Rectangle.java
class Rectangle {
    int length, breadth;    //instnace variable 
    boolean flag;
    void setValues()
    {
    	length=9;
    	breadth=10;
    }

    void showArea() {
        System.out.println(breadth* length+ " "+flag);
        
    }
}



//RectangleMain.java
public class RectangleMain {
 public static void main(String[] args) {
     Rectangle obj = new Rectangle();  
     obj.setValues();
     obj.showArea();
 }
}
