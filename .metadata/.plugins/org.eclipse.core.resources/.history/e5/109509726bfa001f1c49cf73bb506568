// Rectangle.java
class Rectangle {
    int length, breadth;  // Instance variables

    // Method to show the values of length and breadth
    void show() {
        System.out.println("Length: " + length);
        System.out.println("Breadth: " + breadth);
    }
}


//RectangleMain.java
public class RectangleMain {
 public static void main(String[] args) {
     Rectangle obj = new Rectangle();  // Creating an object of Rectangle
     obj.length = 10;  
     obj.breadth = 5;
     obj.show();  // Calling the show() method
 }
}
