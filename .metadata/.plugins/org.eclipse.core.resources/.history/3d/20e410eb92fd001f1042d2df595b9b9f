package com.pack2;

class Example {  
    final int value; // Not initialized here

    Example(int val) {
        value = val; 
    }
}

public class finalkeyword { 
    public static void main(String[] args) {
        Example obj = new Example(100);
        System.out.println("Final value: " + obj.value); // âœ… Access final variable
    }
}
